{
    "builds": 
    [
        {
            "src":"BourStudios/wsgi.py",
            "use":"@vercel/python",
            "config":
            {
                "runtime": "python3.7"
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "BourStudios/wsgi.py"
        },
        {
            "src": "/galsen/static/(.*)",
            "dest": "/galsen/static/$1"
         }
    ]
}