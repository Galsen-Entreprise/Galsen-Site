{
    "builds": [{
        "src":"BourStudios/wsgi.py",
        "use":"@vercel/python",
        "confib":{
            "maxLambdaSize":"15mb",
            "runtime": "python3"
        }
    }],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "BourStudios/wsgi.py"
        }
    ]
}