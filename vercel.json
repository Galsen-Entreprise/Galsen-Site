{
    "builds": 
    [
        {
            "src":"BourStudios/wsgi.py",
            "use":"@vercel/python",
            "config":
            {
                "runtime": "python3.7"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": 
            {
              "distDir": "staticfiles_build"
            }
          }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "BourStudios/wsgi.py"
        },
        {
            "src": "/galsen/static/(.*)",
            "dest": "/galsen/static/$1"
         }
    ]
}